<%= form_for @project do |f| %>
    <p>
      <%= f.label :name %><br>
      <%= f.text_field :name %>
    </p>

    <p>
      <%= f.label :description %><br>
      <%= f.text_area :description %>
    </p>
    <p>
      <%= label_tag :moderator_email%><br>
    <%= text_field_tag :moderator_email %>
    </p>
    <p>
      <%= label_tag :moderator_password%><br>
      <%= text_field_tag :moderator_password %>
    </p>

    <p>
      <%= f.submit %>
    </p>
<% end %>